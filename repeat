#include <stdio.h>

void imprimirNumerosPares(int max_repeat) {
    int i;
    
     for(i = 0; i <= max_repeat ; i++ ) {
        if (i % 2 == 0) {
            printf("%d ", i);
        } 
     }
}

int main() {
    int quanidadeRepeticao;
    
    printf("Informe a quantidade de repeticoes: ");
    scanf("%d", &quanidadeRepeticao);
   
    imprimirNumerosPares(quanidadeRepeticao);
    
   
        
    return 0;
}
